{
	"name": "bun-express",
	"module": "index.ts",
	"type": "module",
	"private": true,
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@commitlint/cli": "^20.3.1",
		"@commitlint/config-conventional": "^20.3.1",
		"@types/bun": "latest",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7"
	},
	"scripts": {
		"dev": "bun run src/index.ts",
		"prepare": "husky",
		"bioime": "bunx biome check --write"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@types/express": "^5.0.6",
		"express": "^5.2.1"
	},
	"lint-staged": {
		"*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
			"biome check --write --no-errors-on-unmatched"
		]
	}
}
